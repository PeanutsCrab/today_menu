<% content_for(:title, t('.title')) %>
<% breadcrumb :likes_menus_path %>
<div class="container pt-3">
  <!-- 掲示板一覧 -->
  <div class="row">
    <div class="col-12">
      <div class="row">
        <% if @like_menus.present? %>
          <% @like_menus.each do |menu| %>
            <%= render 'menu', menu: menu %>
          <% end %>
        <% else %>
          <p><%= t('.no_result') %></p>
        <% end %>
      </div>
      <%= paginate @like_menus, theme: 'bootstrap-5' %>
    </div>
  </div>
</div>